{
  "name": "ha-mongo-client",
  "version": "1.0.3",
  "description": "High availaility for MongoClient",
  "main": "dist/ha_mongo_client.js",
  "scripts": {
    "test": "./node_modules/.bin/ava",
    "build": "./node_modules/.bin/babel ./src --out-dir ./dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:antipin/ha-mongo-client.git"
  },
  "author": "Alexander I. Antipin",
  "license": "MIT",
  "dependencies": {
    "mongodb": "2.1.21"
  },
  "devDependencies": {
    "ava": "0.15.1",
    "babel-cli": "6.26.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "mongodb-topology-manager": "1.0.6"
  },
  "engines": {
    "node": ">=5.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "ava": {
    "failFast": false,
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
